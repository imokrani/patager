		</navigation-case>
		<navigation-case>
			<from-outcome>IA</from-outcome>
			<to-view-id>/ia.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>CC</from-outcome>
			<to-view-id>/cc.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>ROP</from-outcome>
			<to-view-id>/rop.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>secondeAuthentification</from-outcome>
			<to-view-id>/secondeAuthentification.xhtml</to-view-id>
		</navigation-case>
